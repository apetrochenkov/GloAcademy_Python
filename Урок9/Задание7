hour = 24
minute = 60
second = 60
for g in range(hour):
    if g < 23:
        g += 1
    else:
        break
    for j in range(minute):
        if j < 59:
            j += 1
        else:
            break
        for i in range(second):
            while i <= 59:
                i += 1
                break
            print(g - 1, ':', j - 1, ':', i - 1)
# а может так 
for g in range(24):
    for j in range(60):
        for i in range(60):
            print(g, ':', j, ':', i)

