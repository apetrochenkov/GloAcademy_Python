print('введите первое число')
a = int(input())
print('введите второе число')
b = int(input())
c = a * b
d = a / b
e = a // b
f = a % b
g = a ** b
print(a, ' умноженное на', b, ' равно ', c)
print(a, ' деленное на', b, ' равно ', d)
print(a, ' нацело деленное на', b, ' равно ', e)
print('остаток от деления ', a, 'на ', b, 'равен ', f)
print(a, ' в степени ', b, 'равно ', g)

